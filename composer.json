{
  "name": "umbrellio/laravel-common-objects",
  "license": "MIT",
  "type": "library",
  "description": "Common objects for extending base Laravel functionality.",
  "minimum-stability": "stable",
  "keywords": [
    "laravel",
    "php",
    "helpers",
    "validators",
    "presenters",
    "decorators",
    "rules",
    "validation"
  ],
  "authors": [
    {
      "name": "Korben Dallas",
      "email": "pvsaintpe@umbrellio.biz"
    }
  ],
  "require": {
    "php": "^7.2|^7.3|^7.4|^8.0",
    "ext-json": "*",
    "laravel/framework": "^5.8|^6.0|^7.0|^8.0|^9.0",
    "hemp/presenter": "^2.0"
  },
  "require-dev": {
    "umbrellio/code-style-php": "^1.0",
    "orchestra/testbench": "^3.5|^4.0|^5.0|^6.0",
    "symplify/easy-coding-standard":  "^9.3.15",
    "php-coveralls/php-coveralls": "^2.1"
  },
  "scripts": {
    "lint": [
      "ecs check --config=ecs.php . --fix"
    ]
  },
  "autoload": {
    "psr-4": {
      "Umbrellio\\Common\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Umbrellio\\Common\\tests\\": "tests/"
    }
  }
}
